<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN" "http://www.gwtproject.org/doctype/2.5.1/gwt-module.dtd">
<module>

	<!-- Inherit DefaultWidgetSet -->
	<inherits name="com.vaadin.DefaultWidgetSet" /> 

	<replace-with class="org.tltv.gantt.client.BgGridSvgElement">
    	<when-type-is class="org.tltv.gantt.client.BgGridCssElement" />
    	<any>
      		<when-property-is name="user.agent" value="safari"/>
    	</any>
  	</replace-with>
  	
	<!-- set-property name="user.agent" value="gecko1_8"/-->
</module>
